{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/images/(.*)",
      "dest": "server/static/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "server/index.ts"
    }
  ]
}